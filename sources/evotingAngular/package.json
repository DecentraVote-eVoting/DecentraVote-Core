  "scripts": {
    "ng": "ng",
    "start": "ng serve --base-href /app/ --extra-webpack-config webpack.config.js -o --host 0.0.0.0 --port 3999 --configuration=local",
    "start-e2e": "ng serve --base-href /app/ --extra-webpack-config webpack.config.js -o --host 0.0.0.0 --port 3999 --configuration=e2e",
    "start-production": "ng serve --base-href /app/ --extra-webpack-config webpack.config.js -o --host 0.0.0.0 --port 3999 --configuration=production",
    "build": "ng build --base-href /app/ --extra-webpack-config webpack.config.js --configuration=local",
    "build-production": "ng build --base-href /app/ --extra-webpack-config webpack.config.js --configuration=production",
    "test": "ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "npx cypress open --config watchForFileChanges=true",
    "compodoc": "npx compodoc -p tsconfig.json src"
